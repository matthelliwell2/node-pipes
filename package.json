{
  "name": "@matthelliwell/node-pipes",
  "version": "0.0.1-alpha1",
  "description": "",
  "scripts": {
    "build": "npm run compile && npm run lint && npm run test",
    "compile": "tsc",
    "lint": "eslint . --max-warnings 0  --ext .ts",
    "test": "npx jest --silent --runInBand --coverage .*spec.*ts",
    "upgrade": "npx ncu --upgrade"
  },
  "keywords": [
    "async",
    "worker threads",
    "pipes",
    "filters"
  ],
  "author": {
    "name": "Matt Helliwell",
    "email": "me@matthelliwell.org"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthelliwell2/node-pipes.git"
  },
  "dependencies": {
    "@divine/synchronization": "1.2.1",
    "@rauschma/stringio": "1.4.0",
    "async": "3.2.4",
    "denque": "2.1.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@types/async": "3.2.16",
    "@types/jest": "29.2.4",
    "@types/mockdate": "3.0.0",
    "@types/node": "18.11.17",
    "@types/uuid": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-sonarjs": "0.17.0",
    "jest": "29.3.1",
    "mockdate": "3.0.5",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "printWidth": 180,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/SetUp.ts"
    ],
    "testMatch": [
      "**/*spec.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/__mocks__/",
      "src/workers/RouteThread"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
